    "app/components/Screen/ProfileScreen/ProfileScreen.jsx",
    "app/components/Screen/FavouriteScreen/FavouriteScreen.jsx",
    "app/components/Screen/HomeScreen/AppMapView.jsx",
    "app/components/Screen/HomeScreen/HomeScreen.jsx",
    "app/index.jsx"
